func main()
  x = 3;
  y = x!;
  write "El factorial de "; write x; write " es "; 
  write y; write "%n";

  z = 9-x!;
  write "9 - el factorial de "; write x; write " es "; 
  write z; write "%n";

  z = (y-9)!;
  write "El factorial de "; write y; write " - 9 es "; 
  write z;  write "%n";
endfunc